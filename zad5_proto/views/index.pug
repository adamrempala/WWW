html
    head
        title= title
        style
            include style.css
    body
        h1= message
        img(src="https://paczaizm.pl/content/wp-content/uploads/trzeba-natychmiast-zyc-jest-pozniej-niz-sie-wydaje.jpg")
        img(src="https://paczaizm.pl/content/wp-content/uploads/pingwin-teraz-to-ja-juz-nie-chce-isc-do-fryzjera-teraz-chce-wrocic-na-uczelnie.jpg")
        h1="Meme list"
        ul(class="memes-list")
            each meme in memes.mostExpensiveMemes
                li
                   a(href="meme.html?id=" + meme.id) #{meme.name} - kosztuje #{meme.currentPrice}
                   br
                   img(src=meme.url)
